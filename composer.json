{
    "name": "classmarkets/raven-bundle",
    "type": "symfony-bundle",
    "description": "Display sentry event ids on error pages",
    "keywords": [ "symfony", "bundle", "sentry", "raven" ],
    "license": "MIT",
    "authors": [
        {
            "name": "Peter Schultz",
            "email": "peter.schultz@classmarkets.com"
        }
    ],
    "require": {
        "php": ">= 5.3",
        "symfony/framework-bundle": "~2.3",
        "symfony/twig-bundle": "~2.3",
        "raven/raven": "~0.11"
    },
    "require-dev": {
        "mockery/mockery": "~0.9",
        "symfony/browser-kit": "~2.3",
        "symfony/monolog-bundle": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "Classmarkets\\RavenBundle\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
